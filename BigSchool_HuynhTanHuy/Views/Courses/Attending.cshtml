@model BigSchool_HuynhTanHuy.ViewModels.CourseViewModel
@{
    ViewBag.Title = "Attending";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Attending</h2>


<div class="row">
    <ul class="course">
        @foreach (var course in Model.UpcommingCourses)
        {
            <li>
                @course.DateTime - @course.Lecturer.Name

            </li>
        }
    </ul>
</div>